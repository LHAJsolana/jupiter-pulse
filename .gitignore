      { cache: "no-store" }
    );

    const json = await res.json();

    return NextResponse.json({
      count: json?.data?.length ?? 0,
      routes: (json?.data ?? []).map((r) =>
        r.marketInfos?.map((m) => m.label).join(" â†’ ")
      ),
    });
  } catch (e) {
    return NextResponse.json({ error: String(e) }, { status: 500 });
  }
}
